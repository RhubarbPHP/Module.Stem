{
  "name": "rhubarbphp/module-stem",
  "description": "A data modelling and ORM module for the Rhubarb PHP framework",
  "keywords": [
    "modelling",
    "orm",
    "model",
    "data",
    "php",
    "framework"
  ],
  "homepage": "http://www.rhubarbphp.com/",
  "license": "Apache-2.0",
  "autoload": {
    "psr-4": {
      "Rhubarb\\Stem\\": "src/",
      "Rhubarb\\Stem\\Tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/gcdtech/php-usecase-pattern"
    }
  ],
  "config": {
    "bin-dir": "bin/"
  },
  "require": {
    "php": "^7.2",
    "rhubarbphp/rhubarb": "^1.6.8",
    "gcd/usecases": "dev-master"
  },
  "require-dev": {
    "rhubarbphp/custard" : "^1.0.0",
    "codeception/codeception": "^2.1.8"
  }
}

{
  "name": "rhubarbphp/module-stem",
  "description": "A data modelling and ORM module for the Rhubarb PHP framework",
  "keywords": [
    "modelling",
    "orm",
    "model",
    "data",
    "php",
    "framework"
  ],
  "homepage": "http://www.rhubarbphp.com/",
  "license": "Apache-2.0",
  "autoload": {
    "psr-4": {
      "Rhubarb\\Stem\\": "src/",
      "Rhubarb\\Stem\\Tests\\": "tests/"
    }
  },
  "config": {
    "bin-dir": "bin/"
  },
  "require": {
    "rhubarbphp/rhubarb": "1.0.x-dev"
  },
  "require-dev": {
    "rhubarbphp/custard" : "^1.0.4",
    "codeception/codeception": "^2.0",
    "rhubarbphp/module-build-status-updater": "dev-debug as 1.0.3",
    "tan-tan-kanarek/github-php-client": "dev-master as 1.0",
    "phploc/phploc": "*",
    "theseer/phpdox": "*",
    "phpmd/phpmd": "*",
    "squizlabs/php_codesniffer": "*",
    "sebastian/phpcpd": "*",
    "pdepend/pdepend": "*"
  }
}
